{
	"compilerOptions": {
	  "pretty": true,
	  "target": "ES6",
	  "module": "ES6",
	  "moduleResolution": "Node",
	  "importHelpers": false,
	  "experimentalDecorators": true,
	  "emitDecoratorMetadata": true,
	  "declaration": true,
	  "declarationMap": true,
	  "sourceMap": true,
	  "noEmitOnError": true,
	  "strict": true,
	  "noImplicitAny": false,
	  "strictPropertyInitialization": false,
	  "types":["vite/client"],
	  "outDir": "dist/build",
	  "rootDir": "src",
	  "lib": [
		"dom",
		"esnext"
	  ]
	},
	"include": [
	  "src"
	]
  }